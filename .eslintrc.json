{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-console": "error",
    "max-len": ["error", 130],
    "max-lines": ["error", 500],
    "indent": ["error", 2],
    "max-depth": ["error", 3],
    "@typescript-eslint/no-unused-vars": "error",
    "no-param-reassign": "error",
    "no-multiple-empty-lines": "error",
    "prettier/prettier": "warn",
    "no-invalid-this": "off"
  }
}
